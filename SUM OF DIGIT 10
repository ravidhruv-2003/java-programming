import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        int a = sc.nextInt();
        int temp = a;
        int count =0;
        if (a <=99){
            System.out.println("Invalid Input");
        }else {
            
        while (temp>0){
            count+= temp % 10;
            temp= temp/10;
           
        } System.out.println("Sum of digit is "+count);
    }
        
}}
